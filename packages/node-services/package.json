{
  "name": "node-services",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "build:dev": "yarn build",
    "start": "tsc --watch -p ./tsconfig.json",
    "lint": "eslint ./src",
    "test": "yarn lint && jest -c ./jest.config.js",
    "test:watch": "jest -c ./jest.config.js --watch",
    "test:coverage": "jest -c ./jest.config.js && codecov",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "constants-browserify": "^1.0.0",
    "events": "^3.0.0",
    "resolve": "^1.10.0",
    "util": "0.11.1",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/events": "^1.2.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^11.10.5",
    "@types/resolve": "^0.0.8",
    "@types/ws": "^6.0.1",
    "jest": "^24.8.0",
    "typescript": "3.5.2"
  }
}
